<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.duokr.goquicklearn"
    android:versionCode="3"
    android:versionName="1.2" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="net.duokr.goquicklearn.activity.ContentListMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.duokr.goquicklearn.activity.ContentViewActivity"
            android:label="@string/activity_content_view_title"
            android:parentActivityName="net.duokr.goquicklearn.activity.ContentListMainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.duokr.goquicklearn.activity.ContentListMainActivity" />
        </activity>
        <activity
            android:name="net.duokr.goquicklearn.activity.AboutActivity"
            android:label="@string/activity_about_title"
            android:parentActivityName="net.duokr.goquicklearn.activity.ContentListMainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.duokr.goquicklearn.activity.ContentListMainActivity" />
        </activity>
    </application>

</manifest>